# define the root directory for development projects
hostname=`hostname`


# if we're on a development system...
if [ "$hostname" = "CodeHammer" ] || [ "$hostname" = "joker" ];
then
    export NUPIC_PROJ_HOME=~/Documents/Development_Projects/Face_Regonition_Projects
fi

export PYTHONPATH=/opt/predictors/nupic.guides


# activate Python 2.7 environment for use with Nupic
source /opt/python_envs/nupic/bin/activate




